Host github.com
    Hostname github.com
    User git 
    IdentityFile ~/.ssh/keys/github
    AddKeysToAgent yes

Host bitbucket.org
    Hostname bitbucket.org
    User git 
    IdentityFile ~/.ssh/keys/bitbucket
    AddKeysToAgent yes
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedKeyTypes +ssh-rsa
